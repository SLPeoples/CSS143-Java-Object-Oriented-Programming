    public int findSmallest(int[] arr, int begin, int end) {
        //returns the index of the smallest element
        int minIndex = begin;       //hint
        for(int i = begin; i < end ; i++) {
            if(arr[begin - 1] < arr[begin]) {
                minIndex = begin;
            } else 
                minIndex = begin - 1;
        }
        return minIndex;
    }
    
